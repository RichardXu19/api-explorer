{
    "info": {
        "version": "7.3.0-SNAPSHOT", 
        "description": "# Licensing Service API Specification", 
        "title": "Licensing Service API"
    }, 
    "paths": {
        "/api/assets": {
            "get": {
                "responses": {
                    "200": {
                        "description": "successful operation", 
                        "schema": {
                            "$ref": "#/definitions/AssetResources"
                        }
                    }
                }, 
                "summary": "Get all assets", 
                "description": "Retrieves all assets present in the system.", 
                "tags": [
                    "asset"
                ]
            }
        }, 
        "/api/dlfs": {
            "post": {
                "responses": {
                    "201": {
                        "description": ""
                    }, 
                    "200": {
                        "description": "successful operation", 
                        "schema": {
                            "$ref": "#/definitions/Dlf"
                        }
                    }
                }, 
                "summary": "Add DLF", 
                "description": "Add a new DLF to the system.", 
                "parameters": [
                    {
                        "schema": {
                            "$ref": "#/definitions/Dlf"
                        }, 
                        "description": "The DLF to be added.", 
                        "required": true, 
                        "name": "body", 
                        "in": "body"
                    }
                ], 
                "tags": [
                    "dlf"
                ]
            }, 
            "get": {
                "responses": {
                    "200": {
                        "description": "successful operation", 
                        "schema": {
                            "$ref": "#/definitions/DlfResources"
                        }
                    }
                }, 
                "summary": "Get DLFs", 
                "description": "Retrieves all DLFs present in the system.", 
                "tags": [
                    "dlf"
                ]
            }
        }, 
        "/api/dlfs/{id}": {
            "delete": {
                "responses": {
                    "204": {
                        "description": "No Content."
                    }
                }, 
                "summary": "Delete DLF", 
                "description": "Delete a DLF from the system that was added by a user.", 
                "parameters": [
                    {
                        "description": "The UUID of the DLF.", 
                        "pattern": ".+", 
                        "required": true, 
                        "in": "path", 
                        "type": "string", 
                        "name": "id"
                    }
                ], 
                "tags": [
                    "dlf"
                ]
            }, 
            "get": {
                "responses": {
                    "200": {
                        "description": "successful operation", 
                        "schema": {
                            "$ref": "#/definitions/Dlf"
                        }
                    }
                }, 
                "summary": "Get DLF", 
                "description": "Retrieve a DLF.", 
                "parameters": [
                    {
                        "description": "The UUID of the DLF.", 
                        "pattern": ".+", 
                        "required": true, 
                        "in": "path", 
                        "type": "string", 
                        "name": "id"
                    }
                ], 
                "tags": [
                    "dlf"
                ]
            }
        }, 
        "/api/licenses": {
            "post": {
                "responses": {
                    "201": {
                        "description": "Created."
                    }
                }, 
                "summary": "Add serial key", 
                "description": "Adds a serial key to the system.", 
                "parameters": [
                    {
                        "schema": {
                            "$ref": "#/definitions/SerialKeyInfo"
                        }, 
                        "description": "The serial key information to be added.", 
                        "required": true, 
                        "name": "body", 
                        "in": "body"
                    }
                ], 
                "tags": [
                    "licensing"
                ]
            }, 
            "get": {
                "responses": {
                    "200": {
                        "description": "successful operation", 
                        "schema": {
                            "$ref": "#/definitions/LicenseResources"
                        }
                    }
                }, 
                "summary": "Get all licenses", 
                "description": "Retrieves all licenses present in the system.", 
                "tags": [
                    "licensing"
                ]
            }
        }
    }, 
    "schemes": [
        "https"
    ], 
    "tags": [
        {
            "name": "asset"
        }, 
        {
            "name": "dlf"
        }, 
        {
            "name": "licensing"
        }
    ], 
    "basePath": "/component-registry", 
    "definitions": {
        "SerialKeyInfo": {
            "type": "object", 
            "properties": {
                "serialKeys": {
                    "items": {
                        "type": "string"
                    }, 
                    "type": "array"
                }
            }
        }, 
        "Product": {
            "type": "object", 
            "properties": {
                "editionKey": {
                    "type": "string"
                }, 
                "suiteName": {
                    "type": "string"
                }, 
                "id": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }, 
                "family": {
                    "$ref": "#/definitions/ProductFamily"
                }, 
                "name": {
                    "type": "string"
                }
            }
        }, 
        "Dlf": {
            "type": "object", 
            "properties": {
                "dlfProperties": {
                    "additionalProperties": {
                        "type": "string"
                    }, 
                    "type": "object"
                }, 
                "id": {
                    "type": "string", 
                    "format": "uuid"
                }, 
                "dlfEncoded": {
                    "type": "string"
                }
            }
        }, 
        "License": {
            "type": "object", 
            "properties": {
                "assetId": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }, 
                "id": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }, 
                "licenseInfo": {
                    "$ref": "#/definitions/LicenseInfo"
                }
            }
        }, 
        "KeyAnyValue": {
            "type": "object", 
            "properties": {
                "value": {
                    "type": "string"
                }, 
                "key": {
                    "type": "string"
                }
            }
        }, 
        "LicenseProductCapability": {
            "type": "object", 
            "properties": {
                "version": {
                    "type": "string"
                }, 
                "features": {
                    "items": {
                        "$ref": "#/definitions/Feature"
                    }, 
                    "type": "array"
                }, 
                "keyValues": {
                    "items": {
                        "$ref": "#/definitions/KeyAnyValue"
                    }, 
                    "type": "array"
                }
            }
        }, 
        "AssetResources": {
            "xml": {
                "name": "AssetResources"
            }, 
            "type": "object", 
            "properties": {
                "content": {
                    "xml": {
                        "wrapped": true
                    }, 
                    "items": {
                        "$ref": "#/definitions/Asset"
                    }, 
                    "type": "array"
                }, 
                "links": {
                    "xml": {
                        "wrapped": true, 
                        "name": "links"
                    }, 
                    "items": {
                        "$ref": "#/definitions/Link"
                    }, 
                    "type": "array"
                }
            }
        }, 
        "SerialKeyLicenseInfo": {
            "allOf": [
                {
                    "$ref": "#/definitions/LicenseInfo"
                }, 
                {
                    "type": "object", 
                    "properties": {
                        "serialKeys": {
                            "items": {
                                "type": "string"
                            }, 
                            "type": "array"
                        }
                    }
                }
            ]
        }, 
        "LicenseInfo": {
            "type": "object", 
            "properties": {
                "restrictions": {
                    "items": {
                        "$ref": "#/definitions/LicenseProductRestriction"
                    }, 
                    "type": "array"
                }, 
                "name": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }, 
                "expiration": {
                    "type": "string", 
                    "format": "date-time"
                }
            }
        }, 
        "Feature": {
            "type": "object", 
            "properties": {
                "id": {
                    "type": "string"
                }
            }
        }, 
        "LicenseProductRestriction": {
            "type": "object", 
            "properties": {
                "product": {
                    "$ref": "#/definitions/Product"
                }, 
                "licenseProductCapabilities": {
                    "items": {
                        "$ref": "#/definitions/LicenseProductCapability"
                    }, 
                    "type": "array"
                }, 
                "costUnitLimits": {
                    "items": {
                        "$ref": "#/definitions/CostUnitLimit"
                    }, 
                    "type": "array"
                }
            }
        }, 
        "ProductFamily": {
            "type": "object", 
            "properties": {
                "version": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }, 
                "name": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }
            }
        }, 
        "DlfResources": {
            "xml": {
                "name": "DlfResources"
            }, 
            "type": "object", 
            "properties": {
                "content": {
                    "xml": {
                        "wrapped": true
                    }, 
                    "items": {
                        "$ref": "#/definitions/Dlf"
                    }, 
                    "type": "array"
                }, 
                "links": {
                    "xml": {
                        "wrapped": true, 
                        "name": "links"
                    }, 
                    "items": {
                        "$ref": "#/definitions/Link"
                    }, 
                    "type": "array"
                }
            }
        }, 
        "Link": {
            "type": "object", 
            "properties": {
                "href": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }, 
                "rel": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }
            }
        }, 
        "Asset": {
            "type": "object", 
            "properties": {
                "id": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }, 
                "assetInfo": {
                    "$ref": "#/definitions/AssetInfo"
                }
            }
        }, 
        "CostUnitLimit": {
            "type": "object", 
            "properties": {
                "value": {
                    "type": "integer", 
                    "format": "int64"
                }, 
                "unit": {
                    "$ref": "#/definitions/CostUnit"
                }, 
                "enforcementType": {
                    "type": "string"
                }
            }
        }, 
        "LicenseResources": {
            "xml": {
                "name": "LicenseResources"
            }, 
            "type": "object", 
            "properties": {
                "content": {
                    "xml": {
                        "wrapped": true
                    }, 
                    "items": {
                        "$ref": "#/definitions/License"
                    }, 
                    "type": "array"
                }, 
                "links": {
                    "xml": {
                        "wrapped": true, 
                        "name": "links"
                    }, 
                    "items": {
                        "$ref": "#/definitions/Link"
                    }, 
                    "type": "array"
                }
            }
        }, 
        "AssetInfo": {
            "type": "object", 
            "properties": {
                "instanceId": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }, 
                "product": {
                    "$ref": "#/definitions/Product"
                }, 
                "scopeId": {
                    "xml": {
                        "attribute": true
                    }, 
                    "type": "string"
                }
            }
        }, 
        "CostUnit": {
            "type": "object", 
            "properties": {
                "id": {
                    "type": "string"
                }
            }
        }
    }, 
    "swagger": "2.0"
}